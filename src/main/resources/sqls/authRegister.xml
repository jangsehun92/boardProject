<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="auth">
	<insert id="save" parameterType="Map">
		insert into 
			AUTH_TB 
		values(
			AUTH_TB_SEQ.NEXTVAL,
			#{mb_no},
			#{auth_key}, 
			#{auth_type}, 
			'N',
		)
	</insert>
</mapper>